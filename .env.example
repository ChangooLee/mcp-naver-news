# Naver News API 키 설정
# API 키는 Naver Developers(https://developers.naver.com/) 에서 발급받을 수 있습니다.

NAVER_NEWS_API_KEY=your_api_key_here

# API 기본 URL (필요한 경우 변경)
NAVER_NEWS_BASE_URL=https://openapi.naver.com/v1/search/news.json

# 로그 파일 설정
LOG_FILE=naver_news.log
LOG_LEVEL=INFO

# MCP 서버 설정
MCP_SERVER_NAME=mcp-naver-news
MCP_HOST=localhost
MCP_PORT=8000

# 로깅 설정
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL 중 선택

# 캐싱 설정 (선택사항)
ENABLE_CACHE=true
CACHE_EXPIRY=3600  # 캐시 만료 시간(초)